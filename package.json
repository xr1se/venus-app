{
  "name": "victron-venus-metrics",
  "version": "1.7.0",
  "description": "Metric framework for Victron VenusOS",
  "license": "ISC",
  "author": "",
  "dependencies": {
    "@datorama/akita": "^6.1.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "chart.js": "^2.9.4",
    "classnames": "^2.3.0",
    "mqtt": "^4.2.6",
    "node-sass": "^5.0.0",
    "observable-hooks": "^4.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^3.1.1",
    "react-scripts": "4.0.3",
    "rxjs": "^6.6.7",
    "serve": "^11.3.2",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.3",
    "workbox-broadcast-update": "^5.1.3",
    "workbox-cacheable-response": "^5.1.3",
    "workbox-core": "^5.1.3",
    "workbox-expiration": "^5.1.3",
    "workbox-google-analytics": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-range-requests": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "workbox-streams": "^5.1.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.5",
    "@testing-library/dom": "^7.30.3",
    "@types/chart.js": "^2.9.32",
    "@types/enzyme": "^3.10.8",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "cypress": "^6.8.0",
    "enzyme": "^3.11.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "mochawesome": "^6.2.2",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.2.0",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "clean": "rm -rf build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "serve": "serve -s build -l 8000",
    "test:dev": "npm test && cypress run",
    "test:unit": "npm test",
    "test:e2e": "cypress run",
    "test:ci": "npm test && cypress run",
    "generate-e2e-report": "npm run copy-e2e-assets && npm run merge-e2e-results && npm run create-e2e-html-report",
    "copy-e2e-assets": "rm -rf cypress/report && mkdir cypress/report && cp -r cypress/screenshots cypress/videos cypress/report/",
    "merge-e2e-results": "mochawesome-merge cypress/results/*.json > cypress/results/results.json",
    "create-e2e-html-report": "marge cypress/results/results.json -f index.html -o cypress/report --cdn",
    "cy:version": "cypress version",
    "cy:verify": "cypress verify",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">1%",
      "last 2 versions"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node hooks/commit-msg.js $HUSKY_GIT_PARAMS"
    }
  }
}
